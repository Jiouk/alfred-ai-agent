# AI Agent SaaS Platform - Environment Variables

# Database
DATABASE_URL=sqlite:///./ai_agent_saas.db

# Security
SECRET_KEY=your-secret-key-here-change-in-production
FERNET_KEY=your-fernet-key-here-change-in-production
CLIENT_AUTH_TOKEN=shared-token-for-client-scoped-api-calls

# Telegram (Admin notifications)
TELEGRAM_ADMIN_CHAT_ID=your-admin-chat-id

# Bot Pool
LOW_POOL_THRESHOLD=10

# Mailgun (Email)
MAILGUN_API_KEY=your-mailgun-api-key
MAILGUN_DOMAIN=mg.yourdomain.com

# Twilio (VoIP + SMS)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# Stripe (Billing)
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
STRIPE_PUBLISHABLE_KEY=pk_test_...

# OpenClaw (LLM Runtime)
OPENCLAW_API_URL=http://localhost:8080
OPENCLAW_API_KEY=your-openclaw-api-key

# Admin
ADMIN_API_KEY=your-admin-api-key

# Application
BASE_URL=https://api.yourdomain.com
DEBUG=false
ENVIRONMENT=production

# Credits
STARTER_CREDITS=50
STARTER_PLAN_CREDITS=500
GROWTH_PLAN_CREDITS=2000
PAYG_CREDITS_PER_10=300

# Plans (monthly)
STARTER_PLAN_PRICE=1900
GROWTH_PLAN_PRICE=4900
PAYG_PACK_PRICE=1000

# Credit Costs
COST_TELEGRAM_MSG=1
COST_EMAIL=2
COST_VOIP_PER_MIN=2
COST_SMS=1
COST_WEB_WIDGET=1

# Feature Flags
ENABLE_TELEGRAM=true
ENABLE_EMAIL=true
ENABLE_VOIP=true
ENABLE_SMS=true
